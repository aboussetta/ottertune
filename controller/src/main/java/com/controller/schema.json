{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "collector output",
  "description": "Collected metrics or knobs info",
  "type": "object",
  "properties": {
    "global" : {
      "type": ["object", "null"],
      "patternProperties" : {
        "^[a-z]" : {
          "type" : "object",
          "patternProperties" : {
            "^[a-z]" : {
              "type" : "string"
            }
          }
        }
      }
    },
    "local" : {
      "type": ["object", "null"],
      "patternProperties" : {
        "^[a-z]" : {
          "type" : "object",
          "patternProperties" : {
            "^[a-z]" : {
              "type" : "object",
              "patternProperties" : {
                "^[a-z]" : {
                  "type" : "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "required": ["global", "local"]
}